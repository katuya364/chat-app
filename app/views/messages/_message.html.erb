<div class="message">
  <div class="upper-message">
    <div class="chat-user">
      <%= message.user.name %>
    </div>
    <div class="daytime">
      <%= message.created_at %>
    </div>
  </div>
  <div class="lower-message">
    <div class="message-content">
      <%= message.content %>
    </div>
  </div>
</div>
